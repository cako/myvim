\documentclass[11pt,a4paper]{article}
\usepackage[portuguese]{babel}
\usepackage[utf8]{inputenc}
\usepackage{a4wide}
\usepackage{amsmath}
\newcommand{\ut}[1]{\underset{\widetilde{}}{\mathbf{#1}}\phantom{}}

\begin{document}
\[V = 
    \begin{cases}
        V_2 +(V_1 - V_2)\frac{r}{r_0}, & r < r_0 \\
        V_1 & r \geq r_0
    \end{cases}
\]

\[V = 
    \begin{cases}
        V_2, & \textrm{inside square with side } \ell_0 \\
        V_1, & \textrm{otherwise}
    \end{cases}
\]

\[ \ut{G} \mathbf{m} = \mathbf{d} \]

\[ \mathbf{m} = \ut{G}_g^{-1} \mathbf{d} \]

\[ \textrm{rms misfit} = \left(\frac{1}{M} \sum_{i = 1}^M (m_i - m_i^0)^2 \right)^{1/2} \]

\begin{align}
\textrm{roughness} & =  \sum_{j = 1}^{M_y}      \sum_{k = 2}^{M_x - 1} (m_{i_0 - 1}   - 2m_{i_0} + m_{i_0 + 1}  )^2    \\   
                   & +  \sum_{j = 2}^{M_y - 1}  \sum_{k = 1}^{M_x}     (m_{i_0 - M_x} - 2m_{i_0} + m_{i_0 + M_x})^2
\end{align}

\[ \textrm{rms residual} = \left(\frac{1}{N} \sum_{i = 1}^N (t_i^c - t_i^0)^2 \right)^{1/2} \]

\[\ut{R} = \ut{G}_g^{-1}\ut{G}\]

\[\ut{C} = \ut{G}_g^{-1}(\ut{G}_g^{-1})^T\]

\[\textrm{spread} = \sum_{i = 1}^M \sum_{j = 1}^M (R_{ij} - \delta_{ij})^2\]

\[c =  \sum_{i = 1}^M C_{ii}\]

\[\ut{G}_g^{-1} = (\ut{G}^T\ut{G} + \varepsilon^2\ut{I})^{-1}\ut{G}^T\] 

\[\ut{G}_g^{-1} = \ut{S}(\ut{G}^T\ut{G} + \varepsilon^2\ut{I})^{-1}\ut{G}^T\] 

\[\ut{G}_g^{-1} = \ut{W}(\ut{W}\ut{G}^T\ut{G}\ut{W}^T + \varepsilon^2\ut{I})^{-1}\ut{W}\ut{G}^T\] 

\[\ut{G}_g^{-1} = (\ut{G}^T\ut{G} + \lambda\ut{D}^T\ut{D})^{-1}\ut{G}^T\]

\[\ut{G}^T\ut{R}\ut{G}\mathbf{m} = \ut{G}^T\ut{R}\mathbf{d}\]

\[\varepsilon^2 = \frac{\sigma_n^2}{\sigma_m^2}\]
\[V = 
    \begin{cases}
        V_2 +(V_1 - V_2)\frac{r}{r_0}, & r < r_0 \\
        V_1 & r \geq r_0
    \end{cases}
\]

\[V = 
    \begin{cases}
        V_2, & \textrm{inside square with side } \ell_0 \\
        V_1, & \textrm{otherwise}
    \end{cases}
\]

\[ \ut{G} \mathbf{m} = \mathbf{d} \]

\[ \mathbf{m} = \ut{G}_g^{-1} \mathbf{d} \]

